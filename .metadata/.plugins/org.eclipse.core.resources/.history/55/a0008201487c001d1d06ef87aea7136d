package com.innovento.Repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import com.innovento.model.UniversityCollegeRelation;

@Repository
public interface UniversityCollegeRelationRepo extends JpaRepository<UniversityCollegeRelation, Long> {

	@Query(value = "select cm.college_name from college_master cm ,university_college_relation ucr where ucr.college_id = cm.college_id and ucr.university_id = 'U-0006'  group by cm.college_name", nativeQuery = true)
	public List<Object>  getCollegeNamesList();

	
}
