package com.innovento.Repository;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import com.innovento.model.UniversityLogin;

@Repository
public interface UniversityRepo extends JpaRepository<UniversityLogin, Long>{
	
	public UniversityLogin findByName(String name);

	@Query(value = "SELECT u.id,u.u_id FROM university_login_master u", nativeQuery = true)
	public List<Object>  getUniversityNamesList();

}
